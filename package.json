{
  "name": "repeat-site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start"
  },
  "dependencies": {
    "@headlessui/react": "^1.2.0",
    "@heroicons/react": "^1.0.1",
    "@netlify/plugin-nextjs": "github:netlify/netlify-plugin-nextjs#fix/default-publish-dir",
    "@tailwindcss/aspect-ratio": "^0.2.0",
    "@tailwindcss/forms": "^0.2.1",
    "@tailwindcss/typography": "^0.3.1",
    "@tailwindcss/ui": "^0.6.2",
    "antd": "^4.9.3",
    "autoprefixer": "^10.2.6",
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "json2csv": "^5.0.6",
    "moment-timezone": "^0.5.32",
    "next": "10.2.3",
    "nprogress": "^0.2.0",
    "postcss": "^8.2.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "^7.2.2",
    "react-slick": "^0.28.1",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "sass-loader": "^12.1.0",
    "slugify": "^1.5.0",
    "tailwindcss": "^2.0.2",
    "tailwindcss-filters": "^3.0.0"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^3.0.0",
    "eslint": "^7.18.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "next-unused": "0.0.6",
    "react-axe": "^3.5.4"
  },
  "eslintConfig": {
    "plugins": [
      "react",
      "eslint-plugin-jsx-a11y",
      "jsx-a11y"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:import/errors",
      "plugin:import/warnings",
      "plugin:jsx-a11y/recommended"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "jest": true
    },
    "rules": {
      "no-debugger": "off",
      "no-console": "off",
      "no-unused-vars": "warn",
      "react/prop-types": "warn"
    },
    "settings": {
      "react": {
        "version": "detect"
      },
      "import/resolver": {
        "node": {
          "paths": [
            "src"
          ]
        }
      }
    },
    "root": true
  },
  "author": "Uri Maxima",
  "license": "ISC"
}
